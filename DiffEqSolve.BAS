110 INPUT  PROMPT "ENTER DELTA X: ": DX
120 REM DEFINE VALUE OF DELTA X
130 INPUT  PROMPT "ENTER Y(0): ": INIT
140 REM GIVE BOUNDARY CONDITION
150 SET WINDOW 0,10,0,10
160 DRAW GRID
170 PLOT POINTS: 0,INIT
180 SET POINT STYLE 3 
190 LET A = INIT
200 LET X = 0
210 FOR I = 0 TO 10/DX
220    LET A = A + DX/A
230    LET X = X + DX
240    PLOT POINTS: X,A
250 NEXT I
310 PRINT "GOODBYE VINAM" 
320 END
  
